{
  "name": "angular2-tour-of-heroes",
  "version": "0.0.1",
  "scripts": {
    "//": "****change to webpack.config.js or delete the config option if you want to run it trhrough VS**** ",
    "builddev:iis": "del-cli node_modules\\ng2-datetime\\node_modules && webpack --config config\\webpack.dev.iis.js",
    "builddev:wds": "del-cli node_modules\\ng2-datetime\\node_modules && webpack --config config\\webpack.dev.wds.js",
    "build": "npm run builddev:iis",
    "ci": "npm run lint",
    "postinstall": "typings install && npm run copyShims && npm run copyPolyfill && npm run copySystem && npm run copyRx && npm run copyBootstrapCss && npm run copyBootstrapDatepickerCss && npm run copyBootstrapDatepickerJs",
    "start": "webpack-dev-server --config config/webpack.dev.WDS.js --inline --hot",
    "typings": "typings",
    "copyBootstrapCss": "copy node_modules\\bootstrap\\dist\\css\\bootstrap.min.css libs",
    "copyBootstrapDatepickerCss": "copy node_modules\\bootstrap-datepicker\\dist\\css\\bootstrap-datepicker3.min.css libs",
    "copyBootstrapDatepickerJs": "copy node_modules\\bootstrap-datepicker\\dist\\js\\bootstrap-datepicker.js libs",
    "copyShims": "copy node_modules\\es6-shim\\es6-shim.min.js libs",
    "copyPolyfill": "copy node_modules\\reflect-metadata\\reflect.js libs && copy node_modules\\zone.js\\dist\\zone.js libs",
    "copySystem": "copy node_modules\\systemjs\\dist\\system.js libs",
    "copyRx": "copy node_modules\\rxjs\\bundles\\Rx.js libs"
  },
  "dependencies": {
    "@angular/common": "2.2.0",
    "@angular/compiler": "2.2.0",
    "@angular/core": "2.2.0",
    "@angular/forms": "2.2.0",
    "@angular/http": "2.2.0",
    "@angular/platform-browser": "2.2.0",
    "@angular/platform-browser-dynamic": "2.2.0",
    "@angular/router": "3.2.0",
    "@angular/upgrade": "2.2.0",
    "@angularclass/hmr": "1.2.2",
    "@angularclass/hmr-loader": "3.0.2",
    "angular2-in-memory-web-api": "0.0.14",
    "bootstrap": "^3.3.6",
    "bootstrap-datepicker": "^1.6.4",
    "bootstrap-timepicker": "^0.5.2",
    "concurrently": "^2.2.0",
    "core-js": "^2.4.1",
    "del-cli": "^0.2.0",
    "es6-shim": "^0.35.1",
    "jquery": "^3.1.0",
    "lite-server": "^2.2.2",
    "ng2-bootstrap": "^1.1.5",
    "ng2-datetime": "^1.2.0",
    "reflect-metadata": "^0.1.8",
    "rxjs": "^5.0.0-beta.12",
    "typescript": "^2.1.0-dev.20160919",
    "zone.js": "^0.6.26"
  },
  "devDependencies": {
    "@angularclass/hmr": "^1.2.2",
    "@types/core-js": "^0.9.32",
    "angular2-template-loader": "^0.5.0",
    "awesome-typescript-loader": "^2.2.4",
    "concurrently": "^2.0.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.26.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.22.0",
    "raw-loader": "^0.5.1",
    "script-ext-html-webpack-plugin": "^1.3.2",
    "source-map-loader": "^0.1.5",
    "to-string-loader": "^1.1.5",
    "ts-loader": "^0.8.2",
    "typescript": "^2.0.0",
    "typings": "^0.7.12",
    "url-loader": "^0.5.7",
    "webpack": "2.1.0-beta.27",
    "webpack-dev-middleware": "1.6.1",
    "webpack-dev-server": "2.1.0-beta.11",
    "webpack-md5-hash": "0.0.5",
    "webpack-merge": "~1.0.1"
  },
  "-vs-binding": {
    "BeforeBuild": [
      "tsc"
    ]
  }
}
